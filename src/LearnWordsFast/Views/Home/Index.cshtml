@using LearnWordsFast.DAL.Models
@model List<Word>

@{
    ViewBag.Title = "Lear Words Fast";
}

    <ul>
        @{
            foreach (var word in Model)
            {
                <li>@word.Original -> @word.Translation</li>
            }
        }
    </ul>
<a href="@Url.Action("Add", "Word")">Add word</a>
@Html.ActionLink("Practice words", "Practice", "Word")